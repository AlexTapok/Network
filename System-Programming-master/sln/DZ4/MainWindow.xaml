<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525"
        Loaded="Window_Loaded_1"
        KeyDown="Window_KeyUp_1">
    <Grid>
        <StackPanel>
            <Button Name="start" Height="20" Width="100" Content="Start" Click="start_Click_1"/>
            <Button Name="stop" Height="20" Width="100" Content="Stop" Click="stop_Click_1"/>
            <Button Name="get" Height="20" Width="100" Content="Get" Click="get_Click_1"/>
            <Button Name="loadDll" Height="20" Width="100" Content="Load .dll" Click="loadDll_Click_1"/>
            <Button Name="child1" Height="20" Width="100" Content="Child 1" Click="child1_Click_1"/>
            <Button Name="child2" Height="20" Width="100" Content="Child 2" Click="child2_Click_1"/>
            <TextBox Name="pressedButton" Height="20" Width="100" IsEnabled="False" />
            <TextBox Name="changeChild1" Height="50" TextChanged="changeChild1_TextChanged_1"/>
            <TreeView Name="processesTree" Loaded="processesTree_Loaded_1" ScrollViewer.CanContentScroll="True" Height="120" />
        </StackPanel>
    </Grid>
</Window>
